<filter-conf>
    <version>2019122817</version>
    <isOpen>1</isOpen>
    <filter-name>sys_high_temp_protect</filter-name>
    <HighTemperatureProtectSwitch>true</HighTemperatureProtectSwitch> 
    <HighTemperatureShutdownSwitch>true</HighTemperatureShutdownSwitch>
    <HighTemperatureFirstStepSwitch>true</HighTemperatureFirstStepSwitch>
    <CountBatteryChangedForFosUpdateTime>30</CountBatteryChangedForFosUpdateTime>
    <HighTemperatureShutdownUpdateTime>15</HighTemperatureShutdownUpdateTime>
    <HighTemperatureProtectFirstStepIn>480</HighTemperatureProtectFirstStepIn>
    <HighTemperatureProtectFirstStepOut>460</HighTemperatureProtectFirstStepOut>
    <HighTemperatureProtectThresholdIn>530</HighTemperatureProtectThresholdIn>
    <HighTemperatureProtectThresholdOut>510</HighTemperatureProtectThresholdOut>
    <HighTemperatureProtectShutDown>750</HighTemperatureProtectShutDown>
    <MediumTemperatureProtectThreshold>10000</MediumTemperatureProtectThreshold>
    <HighTemperatureDisableFlashSwitch>true</HighTemperatureDisableFlashSwitch>
    <HighTemperatureDisableFlashLimit>440</HighTemperatureDisableFlashLimit>
    <HighTemperatureEnableFlashLimit>430</HighTemperatureEnableFlashLimit>
    <HighTemperatureDisableFlashChargeSwitch>true</HighTemperatureDisableFlashChargeSwitch>
    <HighTemperatureDisableFlashChargeLimit>440</HighTemperatureDisableFlashChargeLimit>
    <HighTemperatureEnableFlashChargeLimit>430</HighTemperatureEnableFlashChargeLimit>
    <camera_temperature_limit>420</camera_temperature_limit>
    <HighTemperatureControlVideoRecordSwitch>true</HighTemperatureControlVideoRecordSwitch>
    <HighTemperatureDisableVideoRecordLimit>490</HighTemperatureDisableVideoRecordLimit>
    <HighTemperatureEnableVideoRecordLimit>470</HighTemperatureEnableVideoRecordLimit>
    <DoubleCheckTime>50000</DoubleCheckTime>
    <ToleranceFirstStepIn>500</ToleranceFirstStepIn>
    <ToleranceFirstStepOut>480</ToleranceFirstStepOut>
    <ToleranceSecondStepIn>530</ToleranceSecondStepIn>
    <ToleranceSecondStepOut>510</ToleranceSecondStepOut>
    <ToleranceThreshold>10</ToleranceThreshold>
    <ToleranceStart>350</ToleranceStart>
    <ToleranceInterval>30000</ToleranceInterval>
    <ExceptTemperature>370</ExceptTemperature>
    <ToleranceStop>370</ToleranceStop>
	<HighTemperatureProtect720PVideoSwitch>true</HighTemperatureProtect720PVideoSwitch>
	<HighTemperatureProtect1080PVideoSwitch>true</HighTemperatureProtect1080PVideoSwitch>
	<HighTemperatureFinishCameraLimit>490</HighTemperatureFinishCameraLimit>
</filter-conf>